{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Modify Simulation",
    "description": "Schema for the simulation modification",
    "type": "object",
    "properties": {
        "numPatients": {
            "type": "integer",
            "minimum": 0
        },
        "numDoctors": {
            "type": "integer",
            "minimum": 0
        },
        "numMachines": {
            "type": "integer",
            "minimum": 0
        }
    },
    "required": [
        "numPatients",
        "numDoctors",
        "numMachines"
    ]
}